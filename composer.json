    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gleys\\Notification\\": "src/"
        }
    },
    "authors": [
        {
            "name": "GleysonAndrade",
            "email": "gleysonsystec@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "phpmailer/phpmailer": "6.8.0"
    },
    "config": {
        "vendor-dir": "lib_ext"
    },
    "autoload": {
        "psr-4": {
            "Notification\\": "_app"
        }
    }
}
